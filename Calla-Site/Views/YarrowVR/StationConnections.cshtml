@{
    Layout = "_YarrowVRAdminPage";
    ViewData["Title"] = "Station Connections";
}

@model System.Collections.Generic.IEnumerable<Yarrow.Data.StationConnections>


@if (Model == null || Model.Empty())
{
    <p>No Content</p>
}
else
{
    <table id="output" cellspacing="0">
        <thead>
            <tr>
                <th>From</th>
                <th>To</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var row in Model)
            {
                <tr>
                    <td><a href="~/YarrowVR/Station/@row.FromStationId">@row.FromStationId</a></td>
                    <td><a href="~/YarrowVR/Station/@row.ToStationId">@row.ToStationId</a></td>
                </tr>
            }
        </tbody>
    </table>
}