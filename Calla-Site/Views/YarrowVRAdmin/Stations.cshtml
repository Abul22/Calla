@{
    Layout = "_YarrowVRAdminPage";
    ViewData["Title"] = "Stations";
}

@model System.Collections.Generic.IEnumerable<Yarrow.Data.Stations>


@if (Model == null || Model.Empty())
{
    <p>No Content</p>
}
else
{
    <table id="output" cellspacing="0">
        <thead>
            <tr>
                <th>Transform</th>
                <th>File</th>
                <th>Latitude</th>
                <th>Longitude</th>
                <th>Altitude</th>
                <th>Rotation</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var row in Model)
            {
                <tr>
                    <td><a href="~/YarrowVRAdmin/Transform/@row.TransformId">@row.TransformId</a></td>
                    <td><a href="~/YarrowVRAdmin/File/@row.FileId">@row.FileId</a></td>
                    <td>@row.Latitude</td>
                    <td>@row.Longitude</td>
                    <td>@row.Altitude</td>
                    <td>@row.Rotation</td>
                </tr>
            }
        </tbody>
    </table>
}